{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/compass/src",
  "monorepo": true,
  "root": "apps/compass",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/compass/tsconfig.app.json"
  },
  "projects": {
    "compass": {
      "type": "application",
      "root": "apps/compass",
      "entryFile": "main",
      "sourceRoot": "apps/compass/src",
      "compilerOptions": {
        "tsConfigPath": "apps/compass/tsconfig.app.json"
      }
    },
    "hourglass": {
      "type": "application",
      "root": "apps/hourglass",
      "entryFile": "main",
      "sourceRoot": "apps/hourglass/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hourglass/tsconfig.app.json"
      }
    },
    "clay": {
      "type": "application",
      "root": "apps/clay",
      "entryFile": "main",
      "sourceRoot": "apps/clay/src",
      "compilerOptions": {
        "tsConfigPath": "apps/clay/tsconfig.app.json"
      }
    },
    "bookmark": {
      "type": "application",
      "root": "apps/bookmark",
      "entryFile": "main",
      "sourceRoot": "apps/bookmark/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bookmark/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    }
  }
}